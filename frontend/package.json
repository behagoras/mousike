{
  "name": "escuelajs-reto-10",
  "version": "1.0.0",
  "description": "Spotify clone in react",
  "main": "server/index.js",
  "scripts": {
    "build": "webpack-cli --config webpack.config.js",
    "start": "node src/server/index.js",
    "start:watch": "webpack-dev-server --open --mode development",
    "start:dev": "nodemon src/server/index.js --exact babel-node",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "front:dev": "webpack-dev-server --open --mode development --config ./src/frontend/webpack.config",
    "front": "webpack-dev-server --open --mode production --config ./src/frontend/webpack.config",
    "storybook": "start-storybook -p 9009 ./src/.storybook/webpack.config",
    "format": "prettier --write '{*.js,src/**/*.{js,jsx}}'",
    "lint": "eslint src/ --fix"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif)$": "<rootDir>/src/frontend/__mocks__/fileMock.js",
      "\\.(scss|sass)$": "<rootDir>/src/frontend/__mocks__/styleMock.js"
    }
  },
  "keywords": [
    "react",
    "express",
    "webpack"
  ],
  "author": "David Behar <david@behar.io>",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "7.6.4",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.6.3",
    "@babel/preset-react": "7.6.3",
    "@babel/register": "7.6.2",
    "@hapi/boom": "8.0.1",
    "@storybook/addon-knobs": "5.2.6",
    "@storybook/addon-storysource": "5.2.3",
    "@storybook/react": "5.2.6",
    "axios": "0.19.0",
    "babel-loader": "8.0.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-assign": "6.22.0",
    "classnames": "2.2.6",
    "cookie-parser": "1.4.4",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "file-loader": "4.2.0",
    "helmet": "3.21.2",
    "history": "4.10.1",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "ignore-styles": "5.0.1",
    "lodash": "4.17.15",
    "md5": "2.2.1",
    "node-spotify-api": "1.1.1",
    "passport": "0.4.0",
    "passport-facebook": "3.0.0",
    "passport-google-oauth": "2.0.0",
    "passport-http": "0.3.0",
    "passport-oauth": "1.0.0",
    "passport-oauth2": "1.5.0",
    "passport-twitter": "1.0.4",
    "prop-types": "15.7.2",
    "react": "^16.11.0",
    "react-dom": "16.11.0",
    "react-hot-loader": "4.12.15",
    "react-redux": "^7.1.1",
    "react-router": "5.1.2",
    "react-router-config": "^5.0.1",
    "react-router-dom": "^5.0.1",
    "redux": "4.0.4",
    "redux-thunk": "2.3.0",
    "styled-components": "4.4.1",
    "webpack": "4.41.2",
    "webpack-cli": "3.3.10"
  },
  "devDependencies": {
    "babel-eslint": "10.0.3",
    "babel-jest": "^24.9.0",
    "css-loader": "3.2.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "5.16.0",
    "eslint-config-airbnb": "17.1.1",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.1",
    "eslint-plugin-react": "7.16.0",
    "husky": "2.7.0",
    "jest": "^24.9.0",
    "lint-staged": "8.2.1",
    "mini-css-extract-plugin": "0.7.0",
    "node-sass": "4.13.0",
    "nodemon": "1.19.4",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.12.0",
    "sass-loader": "8.0.0",
    "style-loader": "0.23.1",
    "stylus": "0.54.7",
    "stylus-loader": "3.0.2",
    "webpack-dev-middleware": "3.7.2",
    "webpack-dev-server": "3.9.0",
    "webpack-hot-middleware": "2.25.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "npm run format",
      "npm run lint",
      "git add"
    ]
  },
  "engines": {
    "node": "^8 || ^10",
    "npm": "6.x"
  }
}
